{% extends "layouts/default.njk" %}
{% block content %}
<body class="post">
    {% include "partials/uio.njk" %}
    <main>
        <nav class="flc-toc-tocContainer"></nav>
        <article>
            <header>
                <h1>{{ title }}</h1>
                <time datetime="{{ date | isoDateFilter }}">{{ date | dateFilter }}</time>
            </header>
            <div class="content">
            {{ content | safe }}
            </div>
        </article>
    </main>
</body>
{% endblock %}
