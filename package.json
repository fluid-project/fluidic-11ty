{
    "name": "fluidic-11ty",
    "version": "1.0.0",
    "description": "Fluid is an open, collaborative project to improve the user experience and inclusiveness of open source software.",
    "scripts": {
        "start": "npm run clean && npm run serve",
        "serve": "eleventy --serve",
        "build": "rimraf dist && eleventy",
        "lint": "grunt lint",
        "clean": "rimraf dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sachin10101998/fluidic-11ty.git"
    },
    "author": "OCAD University",
    "license": "CC-BY-4.0",
    "bugs": {
        "url": "https://github.com/sachin10101998/fluidic-11ty/issues"
    },
    "homepage": "https://github.com/sachin10101998/fluidic-11ty#readme",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "lint-staged": {
        "*.js": [
            "eslint"
        ],
        "*.md": [
            "markdownlint"
        ]
    },
    "dependencies": {
        "@11ty/eleventy": "0.11.0",
        "@11ty/eleventy-plugin-rss": "1.0.7",
        "@11ty/eleventy-plugin-syntaxhighlight": "3.0.1"
    },
    "devDependencies": {
        "@tbranyen/jsdom": "13.0.0",
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "debug": "4.1.1",
        "gpii-grunt-lint-all": "1.0.7",
        "grunt": "1.1.0",
        "grunt-contrib-clean": "2.0.0",
        "grunt-contrib-copy": "1.0.0",
        "html-minifier": "4.0.0",
        "husky": "4.3.0",
        "image-size": "0.8.3",
        "infusion": "3.0.0-dev.20200326T173810Z.24ddb2718",
        "lint-staged": "10.4.0",
        "sanitize-html": "1.24.0",
        "sanitize.css": "11.0.1",
        "slugify": "1.4.0"
    }
}
