{
    "name": "fluidic-11ty",
    "version": "0.1.0",
    "description": "Fluid is an open, collaborative project to improve the user experience and inclusiveness of open source software.",
    "scripts": {
        "build": "npm-run-all -l clean build:*",
        "build:webpack": "cross-env NODE_ENV=production webpack --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build:eleventy": "cross-env NODE_ENV=production eleventy",
        "clean": "rimraf dist",
        "lint": "grunt lint",
        "start": "npm-run-all -l clean -p start:*",
        "start:webpack": "cross-env NODE_ENV=development webpack --config=node_modules/laravel-mix/setup/webpack.config.js --watch",
        "start:eleventy": "cross-env NODE_ENV=development eleventy --serve"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fluid-project/fluidic-11ty.git"
    },
    "author": "OCAD University",
    "license": "BSD-3-Clause",
    "bugs": {
        "url": "https://github.com/fluid-project/fluidic-11ty/issues"
    },
    "homepage": "https://github.com/fluid-project/fluidic-11ty#readme",
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "dependencies": {
        "@11ty/eleventy": "0.11.1",
        "@11ty/eleventy-plugin-rss": "1.0.9",
        "@11ty/eleventy-plugin-syntaxhighlight": "3.0.4",
        "@fluid-project/eleventy-plugin-fluid": "github:fluid-project/eleventy-plugin-fluid#ee75b78"
    },
    "devDependencies": {
        "@commitlint/cli": "11.0.0",
        "@commitlint/config-conventional": "11.0.0",
        "cross-env": "7.0.2",
        "debug": "4.2.0",
        "fluid-grunt-lint-all": "2.0.0",
        "grunt": "1.3.0",
        "html-minifier": "4.0.0",
        "husky": "4.3.0",
        "image-size": "0.9.1",
        "infusion": "3.0.0-dev.20200326T173810Z.24ddb2718",
        "jsdom": "16.4.0",
        "laravel-mix": "5.0.7",
        "move-file": "2.0.0",
        "npm-run-all": "4.1.5",
        "resolve-url-loader": "^3.1.2",
        "rimraf": "3.0.2",
        "sass": "1.28.0",
        "sass-loader": "10.1.0",
        "slugify": "1.4.6",
        "stylus": "0.54.8",
        "stylus-loader": "3.0.2",
        "vue-template-compiler": "2.6.12"
    }
}
